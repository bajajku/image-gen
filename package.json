{
  "name": "image-gen",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:web": "cd ./apps/web && npm run start",
    "start:backend": "cd ./apps/backend && bun run index.ts",
    "generate:db": "cd ./packages/db && npx prisma generate && cd ../.."
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
